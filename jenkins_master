pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Trigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'
                echo '${WORKSPACE}'
                script {
                    def filePath = readFile ${WORKSPACE}/data.txt
                    echo '${filePath}'
              
                }
                
            }
        }
    }
}
